  "scripts": {
    "build": "yarn service build && yarn next build src --no-lint && cp -r ./src/.next ./dist/.next",
    "dev": "yarn next dev src",
    "extract": "formatjs extract '../../fragments/**/src/*.ts*' --out-file locales/ru.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format simple",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@atls-ui-proto/theme": "0.0.1",
    "@atls/next-app-with-body-parser": "0.0.2",
    "@atls/next-config-with-pnp-workspaces": "0.0.2",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@formatjs/cli": "4.7.1",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "js-cookie": "^3.0.1",
    "next": "^12.0.7",
    "next-compose-plugins": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "6.1.0",
    "react-intl": "5.18.1",
    "xorshift": "*"
  },
  "devDependencies": {
    "@identity/error-page": "workspace:0.0.1",
    "@identity/index-page": "workspace:0.0.1",
