[
  {
    "id": "allow-anonymous",
    "version": "v0.0.1",
    "upstream": {
      "url": "http://localhost:4433",
      "preserve_host": true
    },
    "match": {
      "url": "http://<127.0.0.1|localhost>:4455/<.*>",
      "methods": ["GET", "POST", "OPTIONS", "PUT"]
    },
    "authenticators": [
      {
        "handler": "anonymous"
      }
    ],
    "authorizer": {
      "handler": "allow"
    }
  }
]
